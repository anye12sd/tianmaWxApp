/* pages/addressBook/addressBook.wxss */
page{
  background: #f9f9f9;
  height: 100%;
}
.container{
  background: #f9f9f9;
  padding-bottom: 30rpx;
}
.order-detail-img{
  width: 100%;
  height: calc(100vw / 3.87931);
}
.order-detail-box{
  background: #fff;
  padding: 40rpx 60rpx;
  font-size: 30rpx;
  color: #969696;
}
.order-detail-content{
  color: #333;
}
.check-price{
  color: #e60012;
}
.service-box{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.phone-num{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  padding:0;
  font-weight: normal;
}
.service-button{
  font-weight: normal;
  font-size: 28rpx;
  padding: 0;
  height: 80rpx;
  line-height: 80rpx;
}
.phone-num image{
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.service-online image{
  width: 45rpx;
  height: 45rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
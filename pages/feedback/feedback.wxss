/* pages/feedback/feedback.wxss */
page{
  height: 100%;
  background: #f9f9f9;
}
.input-form{
  background: #fff;
  padding-top: 0;
  margin-top: 40rpx;
}
.textarea{
  background: #fff;
  width: calc(100vw - 120rpx);
  margin: 0 auto;
  height:200rpx;
  padding: 20rpx 0;
  overflow-y: auto;
}
.addNewAddress{
  background: #fff;
  padding:20rpx 30rpx;
  position: fixed;
  bottom: 0;
  height:80rpx;
  left:0;
  right:0;
  border-top: 1px solid #e5e5e5;
  z-index: 0
}
.addNewAddress text{
  font-size: 32rpx;
  color: #fff;
  background: #376ee8;
  display: block;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 8rpx;
  width: 100%;
}